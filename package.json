{
  "name": "@brentlintner/vile-sass-lint",
  "version": "0.1.0",
  "description": "A vile plugin for sass-lint",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "compile": "babel -d lib -m common src",
    "compile-dev": "bin/compile-dev",
    "dev": "bin/dev",
    "test": "bin/test",
    "test-cov": "bin/test-cov",
    "vile": "vile -cpd --nodecorations"
  },
  "keywords": [
    "vile",
    "sass",
    "lint",
    "ruby"
  ],
  "dependencies": {
    "@brentlintner/vile": "^0.5.3",
    "bluebird": "^3.1.1",
    "lodash": "^3.10.1",
    "sass-lint": "^1.4.0"
  },
  "devDependencies": {
    "@brentlintner/vile-coffeelint": "0.0.4",
    "@brentlintner/vile-comment": "0.0.4",
    "@brentlintner/vile-coverage": "^0.1.6",
    "@brentlintner/vile-depcheck": "0.0.3",
    "@brentlintner/vile-eclint": "0.0.2",
    "@brentlintner/vile-eslint": "0.0.3",
    "@brentlintner/vile-fs-lint": "^0.1.1",
    "@brentlintner/vile-git": "^0.1.0",
    "@brentlintner/vile-jshint": "0.0.2",
    "@brentlintner/vile-language": "0.0.4",
    "@brentlintner/vile-license": "0.0.3",
    "@brentlintner/vile-ncu": "0.0.4",
    "@brentlintner/vile-newline": "0.0.2",
    "@brentlintner/vile-nsp": "^0.1.0",
    "@brentlintner/vile-semver": "0.0.2",
    "@brentlintner/vile-whitespace": "0.0.3",
    "babel": "^5.8.34",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "coffee-script": "^1.10.0",
    "istanbul": "^0.4.1",
    "mimus": "^0.1.2",
    "mocha": "^2.3.4",
    "release-it": "^2.3.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  }
