{
  "strict": false,
  "browser": true,
  "node": true,
  "asi": true,
  "trailing": true,
  "evil": true,
  "boss": true,
  "expr": true,
  "loopfunc": true,
  "maxstatements": 50,
  "maxcomplexity": 15,
  "maxlen": 10000,
  "maxparams": 7,
  "quotmark": "double",
  "maxdepth": 5,
  "latedef": "nofunc",
  "undef":  "nodefunc",
  "unused": "vars",
  "sub": true,
  "newcap": true,
  "immed": true,
  "indent": 2,
  "eqeqeq": false,
  "eqnull": true,
  "esnext": true,

  "globals": {
      "$": true,
      "require": false,
      "expect": true,
      "describe": false,
      "xdescribe": false,
      "it": false,
      "xit": false,
      "afterEach": false,
      "after": false,
      "beforeEach": false,
      "before": false,
      "Promise": true
  }
}
